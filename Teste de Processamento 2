package teste_processamento;
//Programa pra fazer o calculo de aréa do trapézio.
public class Main {
	//Inicio da Funcão Main.
	public static void main(String[] args) {
	
	double b, B, h, area;// Declaração do tipo das variavéis
	
	//Atribuindo valor as variavéis
	b = 6.0;
	B = 8.0;
	h = 5.0;
	
	area = (b + B) / 2.0 * h;// Calculo 
	
	
	System.out.println(area); // Saida de impressão na tela.

	}

}
